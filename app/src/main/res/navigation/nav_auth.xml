<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_auth"
    app:startDestination="@id/singInFragment">

    <fragment
        android:id="@+id/singInFragment"
        android:name="com.android.chattz.presentation.ui.auth.singin.SingInFragment"
        android:label="SingInFragment"
        tools:layout="@layout/fragment_singin"
        />
    <fragment
        android:id="@+id/singUpFragment"
        android:name="com.android.chattz.presentation.ui.auth.singup.SingUpFragment"
        android:label="SingUpFragment"
        tools:layout="@layout/fragment_singup"
        />

    <action
        android:id="@+id/to_sign_up"
        app:destination="@+id/singUpFragment"
        />
</navigation>